<div class="page-header">
  <h1>
    Tagtorestaurants
    <a href="/tagtorestaurants/new" class="btn btn-lg btn-success">New Tagtorestaurant</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Restaurant</th>
        <th>Actions</th>
      </tr>

      <% @tagtorestaurants.each do |tagtorestaurant| %>
      <tr>
        <td><%= tagtorestaurant.restaurant_id %></td>
        <td>
          <a href="/tagtorestaurants/<%= tagtorestaurant.id %>" class="btn btn-primary">Show</a>
          <a href="/tagtorestaurants/<%= tagtorestaurant.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_tagtorestaurant/<%= tagtorestaurant.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
